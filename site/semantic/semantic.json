{
  "base": "semantic/",
  "paths": {
    "source": {
      "config":      "src/theme.config",
      "definitions": "src/definitions/",
      "site":        "src/site/",
      "themes":      "src/themes/"
    },
    "output": {
      "packaged":     "dist/",
      "uncompressed": "dist/components/",
      "compressed":   "dist/components/",
      "themes":       "dist/themes/"
    },
    "clean": "dist/"
  },
  "permission": false,
  "autoInstall": false,
  "rtl": false,
  "components": ["reset", "site", "container", "divider", "icon", "dropdown", "popup", "visibility"],
  "version": "2.4.2"
}